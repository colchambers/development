      ],
      "no-var": [
        "off"
      ],
      "one-var": [
        "off"
      ],
      "vars-on-top": [
        "off"
      ],
      "no-param-reassign": [
        "off"
      ],
      "no-lone-blocks": [
        "off"
      ],
      "padded-blocks": 0,
      "prefer-template": [
        "off"
      ],
      "prefer-arrow-callback": [
        "off"
      ],
      "default-case": [
        "off"
      ],
      "wrap-iife": [
        2,
        "inside"
      ],
      "no-plusplus": [
        "off"
      ],
      "require-jsdoc": [
        "warn",
        {
          "require": {
            "FunctionDeclaration": true,
            "MethodDefinition": true,
            "ClassDeclaration": true
          }
        }
      ],
      "object-shorthand": [
        "error",
        "never"
      ],
      "space-before-function-paren": "off",
      "strict": "off",
      "valid-jsdoc": [
        "error"
      ]
